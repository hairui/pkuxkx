#class test open;
#alias {path.test} {
	#list test_path create {
		n;e;e;e;s;s;s;s;s;s;se;su;sw;sw;w;s;s;s;wu;eu;su;s;s;
		w;w;w;s;w;s;s;s;s;s;s;e;e;s;n;e;e;n;e;s;n;e;n;n;n;e;s;n;n;n;w;n;w;n;n;s;s;w;w;s;s;s;w;n;s;w;n;s;s;s;e;n;s;e;n;s;s;w;e;n;e;n;n;n;s;e;n;s;w;w;s;s;s;s;s;s;w;w;w;s;n;w;s;n;e;e;e;e;e;
		e;e;w;n;s;w;n;s;w;s;w;e;e;w;s;e;w;sd;s;n;nu;n;n
	};

	#list test_path size test_path_size;
	#var test_path_index 0;
	#ticker {test_walk} {
		#math test_path_index ${test_path_index}+1;
		#if {${test_path_index} > ${test_path_size}} {
			#untick {test_walk};
		};
		#else {
			#list test_path get ${test_path_index} test_path_next;
			${test_path_next};
		};
	} {1};
	#action {这个方向没有出路} {
		#untick {test_walk};
		#showme path error: ${test_path_index}, ${test_path_next};
	};
};
#class test close
