		} {5};
		#action {正等着你呢，上来吧。} {
			genter;
		};
		#action {艄公将一块踏脚板搭上堤岸，} {
			genter;
		};
		#alias {shangchuan} {
			#unticker {sw/yell_boat};
			#unticker {hb_walk};
			#action {所有人被赶下了渡船} {
				#unaction {所有人被赶下了渡船};
				#class guohe.inner kill;
				hb.resume;
			};
		};
		#action {^黄河渡船$} {
			shangchuan;
		};
		#action {^长江渡船$} {
			shangchuan;
		};
		#class guohe.inner close;
	};
	#var hb_paused 1;
	ask shao gong about %1;		
};

#alias {clear_blocker} {
	#var tmp_blocker %1;
	#class clear_blocker.inner open;
	#action {你想杀谁} {
		#class clear_blocker.inner kill;
		#math hb_path_index ${hb_path_index}+1;
		hb.resume;
	};
	#ticker {tmp_blocker_ticker_kill} {
		killall ${tmp_blocker};	
	} {5};
	#class clear_blocker.inner close;
	hb.pause;
	killall ${tmp_blocker};
};

#alias {clear_npc} {
	#var npc_zh %1;
	#class clear_npc.inner open;
	#action {你想杀谁} {
		#var hb_force_wait 0;
	};
	#action {${npc_zh}死了} {
		#var hb_force_wait 0;
	} {1};

	#action {你还是先把对手解决了再说吧} {
		l;
	} {1};
	
	#action {${npc_zh}(%*)} {
		#action {看起来${npc_zh}想杀死你} {
			#var hb_force_wait 1;
		} {1};
		#var npc_en @lower{%%1};
		killall ${npc_en};
    } {1};
	#class clear_npc.inner close;
};

#alias {mx.o} {
	xian mixin;
};

#alias {mx.re} {
	hb.get_owner;
	give mixin to ${hb_owner};
	mx.o;
};

#alias {mx.q} {
	#class mx.inner open;	
	#action {一个伙计挖着鼻屎走了出来，道：你找我啥事} {
		l;
	};
	#action {财物的伙计 %S(%S)} {
		#format {tmp_huoji} {%l} {%%2};
		#class mx.yao.inner open;
		#action {把一包财物砸向你，一转眼不见了。} {
			@setenv{hb_state;GOT_CAIWU};
			show_to_tuiche 拿到财宝;
			play get_item.wav;
			#class mx.yao.inner kill;
			#class mx.inner kill;
			back;
		};
		#action {这里没有这个人} {
			#class mx.yao.inner kill;	
		};
		#ticker {yao} {
			ask $tmp_huoji about yao;
		} {0.5};
		#class mx.yao.inner close;
	} {1};
	#class mx.inner close;
	zhao;
};

