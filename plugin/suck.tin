#class suck open
#alias {suck.start} {
	#alias wi #cr;
	#alias attack #cr;
	unwi;
	bei none;
	gt qzh;
	#math max_nl ${youxiao_neigong} * 15 - 200;
	on_there {
		#ticker {suck} {
			#if {${hpbrief_max_nl} > ${max_nl}} {
				suck.stop;
			};
			#else {
				hit tong ren; maxsuck; halt;
			};
		} {1.5};
		#ticker {chifan} {
			chifan;
		} {30};
	};
};

#alias {suck.stop} {
	#alias wi wi_normal;
	#alias attack attack.normal;
	bei strike;
	#unticker {suck};
	#unticker {chifan};
};
#class suck close