    #if {"${result}" == "NULL"} {
        #format {tmp} {%.6s} {%1};
        #var result @mapget{areas;${tmp}};
        #if {"${result}" == "NULL"} {
            #format {tmp} {%.8s} {%1};
            #var result @mapget{areas;${tmp}};
        };
    };
    #return ${result};
}

